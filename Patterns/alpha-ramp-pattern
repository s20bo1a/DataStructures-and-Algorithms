public class Solution {
    public static void alphaRamp(int n) {
        // Write your code here
      for(int i=0;i<n;i++){
           char k=(char)('A'+i);
          for(int j=0;j<=i;j++){
              
              System.out.print(k +" ");
          }
          System.out.println();
      }


    }
}
